#include <iostream>

using namespace std;

int main() {

    int n, m;

    cout << "Enter the size of square: ";

    cin >> n;

    cout << "Square star pattern: " << endl;

    for (int i = 1; i <= n; i++) {

        for (int j = 1; j <= n; j++) {

            cout << "* ";

        }

        cout << endl;

    }

    cout << "Enter the number of rows and columns of rectangle: ";

    cin >> n >> m;

    cout << "Rectangle dollar pattern: " << endl;

    for (int i = 1; i <= n; i++) {

        for (int j = 1; j <= m; j++) {

            cout << "$ ";

        }

        cout << endl;

    }

    return 0;

}

output
Enter the size of square: 5
Enter the number of rows and columns of rectangle: 4 6
Square star pattern: 
* * * * * 
* * * * * 
* * * * * 
* * * * * 
* * * * * 
Rectangle dollar pattern: 
$ $ $ $ $ $ 
$ $ $ $ $ $ 
$ $ $ $ $ $ 
$ $ $ $ $ $ 

